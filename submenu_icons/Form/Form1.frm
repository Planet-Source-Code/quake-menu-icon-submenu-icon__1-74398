Private Declare Function GetMenu Lib "user32" (ByVal hwnd As Long) As Long
Private Declare Function GetMenuItemID Lib "user32" (ByVal menu As Long, ByVal nPos As Long) As Long
Private Declare Function GetSubMenu Lib "user32" (ByVal menu As Long, ByVal nPos As Long) As Long
Private Declare Function SetMenuItemBitmaps Lib "user32" (ByVal menu As Long, _
  ByVal posisi As Long, ByVal wFlags As Long, ByVal hBitmapUnchecked As Long, _
  ByVal hBitmapChecked As Long) As Long

Const MF_BYPOSITION = &H400&

Private Sub Form_Load()
Dim hMenu As Long, aSubmenu As Long, bSubmenu As Long, POSi As Long
Dim i As Long, x As Long

    hMenu = GetMenu(Me.hwnd)
    '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
    'Column 0 Submenu with subsubmenu - Use MF_BYPOSITION to add icon in subSubmenu
    aSubmenu = GetSubMenu(hMenu, 0)
    For i = 0 To 3
        ' Use Picture Box control
        x = SetMenuItemBitmaps(aSubmenu, i, MF_BYPOSITION, CLng(Picture0(i).Picture), CLng(Picture0(i).Picture))
        ' Use Image List control
        'x = SetMenuItemBitmaps(menu, POSi, 0, CLng(ImageList1.ListImages.Item(i).Picture), CLng(ImageList1.ListImages.Item(i).Picture))
    Next
    
    'Column 0 Submenu 0
    'Here we set the Submenu of the SubMenu - Capturing Submenu 0 and putting it into the Second Submenu 0
    '[ bSubmenu ] Used for setting Sub-Submenu
    '[ GetSubMenu(aSubmenu, 0) ] Used for for setting Submenu
    bSubmenu = GetSubMenu(aSubmenu, 0)
    For i = 0 To 2
        POSi = GetMenuItemID(bSubmenu, i)
        x = SetMenuItemBitmaps(aSubmenu, POSi, 0, CLng(Picture1(i).Picture), CLng(Picture1(i).Picture))
    Next
                
    'SPACE -------------- SPACE
                
    'Column 0 Submenu 2
    'Here we set the Submenu of the SubMenu - Capturing Submenu 0 and putting it into the Second Submenu 2
    '[ bSubmenu ] Used for setting Sub-Submenu
    '[ GetSubMenu(aSubmenu, 2) ] Used for for setting Submenu
    bSubmenu = GetSubMenu(aSubmenu, 2)
    For i = 0 To 2
        POSi = GetMenuItemID(bSubmenu, i)
        x = SetMenuItemBitmaps(aSubmenu, POSi, 0, CLng(Picture2(i).Picture), CLng(Picture2(i).Picture))
    Next
    '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
    'Column 1 Submenu with subsubmenu - Use MF_BYPOSITION to add icon in subSubmenu or 0
    'Column 1  Submenu 1
    aSubmenu = GetSubMenu(hMenu, 1)
    For i = 0 To 3
        x = SetMenuItemBitmaps(aSubmenu, i, MF_BYPOSITION, CLng(Picture3(i).Picture), CLng(Picture3(i).Picture))
    Next

    'Column 0 subSubmenu 1
    'Here we set the Submenu of the SubMenu - Capturing Submenu 0 and putting it into the Second Submenu 1
    '[ bSubmenu ] Used for setting Sub-Submenu
    '[ GetSubMenu(aSubmenu, 1) ] Used for for setting Submenu
    bSubmenu = GetSubMenu(aSubmenu, 1)
    For i = 0 To 1
        POSi = GetMenuItemID(bSubmenu, i)
        x = SetMenuItemBitmaps(aSubmenu, POSi, 0, CLng(Picture4(i).Picture), CLng(Picture4(i).Picture))
    Next


End Sub

